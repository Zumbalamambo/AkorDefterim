<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/coordinatorLayout_FrgSarki"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Beyaz">

    <RelativeLayout
        android:id="@+id/LLSayfa"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <WebView
            android:id="@+id/webview"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <com.github.clans.fab.FloatingActionMenu
            android:id="@+id/FABMenu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="10dp"
            app:menu_icon="@drawable/dots_vertical_beyaz">

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/FABAkorCetveli"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/FABAkorGizleGoster"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/FABTranspozeArti"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/FABTranspozeEksi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/FABPaylas"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/FABVideoKlip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/FABSarkiEkleCikar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/FABSarkiDuzenle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/FABSorunBildir"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </com.github.clans.fab.FloatingActionMenu>
    </RelativeLayout>
</android.support.design.widget.CoordinatorLayout>
